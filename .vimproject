 bin=bin filter="*"{
  build_project.rb
  build_project_tsv.rb
  prepare_prelim_final.sh
 }
 lib=lib {
  sinatra.rb
  pcawg.rb
  rbbt=rbbt{
   entity=entity{
    pcawg.rb
    pcawg=pcawg{
     donor.rb
     specimen.rb
     sample.rb
     study.rb
    }
   }
  }
  pcawg=pcawg{
   sensitive.rb
  }
 }
 test=test {
  test_helper.rb
 }
 www=www{
  views=views{
   tasks.haml
   main.haml
   main=main{
    pie.haml
    study.haml
    study_column.haml
   }
   entity_list=entity_list{
    Gene=Gene{
     study_column.haml
    }
   }
  }
 }
